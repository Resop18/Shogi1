            android:name="edu.up.cs371.resop18.shogi.shogi.ShogiMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.up.cs371.resop18.shogi.shogi.Options">

        </activity>

        <activity android:name="edu.up.cs371.resop18.shogi.shogi.Game">

        </activity>
    </application>

</manifest>